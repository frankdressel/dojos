<Configuration status="error" name="maze" packages="">
    <Appenders>
        <Console name="STDOUT" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss} %p %m%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <Root level="error">
            <AppenderRef ref="STDOUT"/>
        </Root>
        <Logger name="maze" level="info">
            <AppenderRef ref="STDOUT"/>
        </Logger>
    </Loggers>
</Configuration>
